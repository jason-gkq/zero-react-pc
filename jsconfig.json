{
  "compilerOptions": {
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["assets/*"],
      "@src/*": ["src/*"],
      "@common": ["Common"],
      "@common/*": ["Common/*"],
      "@components": ["Common/components"],
      "@components/*": ["Common/components/*"],
      "@utils": ["Common/utils"],
      "@utils/*": ["Common/utils/*"],
      "@redux": ["Common/redux"],
      "@redux/*": ["Common/redux/*"],
      "@menus/*": ["Common/menus/*"],
      "@locales/*": ["Common/locales/*"]
    }
  },
  "exclude": ["node_modules", "dist", "dest"]
}
